<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "../mybatis-3-mapper.dtd" >
<mapper namespace="edu.shanda.dao.EmployeeDAO">
    <select id="modifySaveEmployee">
        insert into t_employee (name,salary,age,images,department_id)
        values (#{name},#{salary},#{age},#{images},#{department_id})
    </select>
</mapper>